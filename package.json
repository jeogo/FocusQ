{
  "name": "focusq",
  "version": "1.0.0",
  "description": "Queue Management System",
  "main": "./out/main/index.js",
  "author": "Focus Team",
  "homepage": "https://github.com/example/electronapp",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "cross-env NODE_ENV=development SCREEN_TYPE=customer electron-vite dev",
    "dev:all": "cross-env NODE_ENV=development LAUNCH_ALL_SCREENS=true electron-vite dev",
    "dev:seq": "cross-env NODE_ENV=development LAUNCH_ALL_SCREENS=true SEQUENTIAL_START=true electron-vite dev",
    "dev:customer": "cross-env NODE_ENV=development SCREEN_TYPE=customer electron-vite dev",
    "dev:display": "cross-env NODE_ENV=development SCREEN_TYPE=display electron-vite dev",
    "dev:employee": "cross-env NODE_ENV=development SCREEN_TYPE=employee COUNTER_ID=1 electron-vite dev",
    "dev:admin": "cross-env NODE_ENV=development SCREEN_TYPE=admin electron-vite dev",
    "build": "electron-vite build",
    "rebuild": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "clean": "rimraf out dist .electron-vite"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "electron-updater": "^6.1.1",
    "express": "^5.1.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.284.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
